{
  "name": "@ordercloud/angular-cms-components",
  "author": "Four51",
  "license": "MIT",
  "version": "1.0.0-beta.8",
  "scripts": {
    "start": "run-s copy-constants buildconfig-dev app-watchmode",
    "build": "run-s copy-constants buildconfig-prod plugin-prod build-lib buildconfig-dev move-scripts build-storybook",
    "copy-constants": "shx cp plugin/src/constants/widget.constants.ts projects/lib/src/shared/constants/widget.constants.ts",
    "move-scripts": "shx cp plugin/dist/plugin.min.js dist/lib/plugin.min.js",
    "app-watchmode": "run-p plugin-dev storybook",
    "storybook": "start-storybook -p 6006",
    "build-lib": "ng build --configuration production",
    "buildconfig-prod": "shx cp -f angular.build.json angular.json",
    "buildconfig-dev": "shx cp -f angular.dev.json angular.json",
    "build-storybook": "build-storybook -c .storybook -o docs && npm run assets-storybook",
    "assets-storybook": "shx cp -r node_modules/tinymce docs",
    "plugin-dev": "webpack --config plugin/webpack/development.config.js",
    "plugin-prod": "webpack --config plugin/webpack/production.config.js",
    "publish-lib": "cd dist/lib && npm publish",
    "publish-please": "publish-please",
    "prepublishOnly": "publish-please guard"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.2",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@storybook/addon-knobs": "^5.3.19",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular-devkit/build-ng-packagr": "~0.1000.1",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.2",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ordercloud/headstart-sdk": "^1.6.3",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/angular": "^5.3.19",
    "@tinymce/tinymce-angular": "^4.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.1",
    "@types/node": "^14.0.22",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.0",
    "case": "^1.6.3",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^6.0.0-next.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "^3.5.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "ngx-spinner": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "ordercloud-javascript-sdk": "4.0.0-12",
    "protractor": "~7.0.0",
    "publish-please": "^5.5.1",
